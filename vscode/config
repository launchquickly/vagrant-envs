Host focal64.local
  HostName 192.168.0.120
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/vagrant/.ssh/id_rsa_focal64
  IdentitiesOnly yes
  LogLevel FATAL

